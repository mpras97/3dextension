{
  "manifest_version": 2,
  "name": "3dextension",
  "description": "3D model extension",
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "favicon.png",
    "default_title": "React Chrome Extension Boilerplate",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["./jquery.js", "background.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "css": [
      "./content-scripts.css"
    ],
    "js": [
      "./jquery.js",
      "./content.js"
    ]
  }],
  "permissions": [
    "<all_urls>",
    "activeTab",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
